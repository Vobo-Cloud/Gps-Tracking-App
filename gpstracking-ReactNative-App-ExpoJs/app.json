{
  "expo": {
    "name": "Vobo Takip",

    "slug": "vobotakip",
    "scheme": "vobotakip",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e45214"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "**/*/*"
    ],
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#e45214"
    },
    "androidStatusBar": {
      "backgroundColor": "#e45214"
    },

    "notification": {
      "icon": "./assets/icon.png"
    },
    "loading": {
      "icon": "./assets/icon.png"
    },
    "android": {
      "versionCode": 2,
      "package": "com.vobo.track",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "LOCATION"
      ]
    }
  }
}